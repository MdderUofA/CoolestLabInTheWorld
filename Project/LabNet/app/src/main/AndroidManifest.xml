<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.lab_net">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LabNet"
        tools:ignore="ExtraText">
        <activity android:name=".plotLocActivity"></activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${GOOGLE_API_KEY}" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserProfile" />
        <activity android:name=".MapActivity" />
        <activity android:name=".Statistics" />
        <activity android:name=".Histogram" />
        <activity android:name=".ExperimentActivity" />
        <activity android:name=".SubscribedExperimentActivity" />
        <activity android:name=".SubscribedUserActivity" />
        <activity android:name=".NonNegativeExperimentActivity" />
        <activity android:name=".MeasurementExperimentActivity" />
        <activity android:name=".CountExperimentActivity" />
        <activity android:name=".BinomialExperimentActivity" />
        <activity android:name=".AnswersActivity" />
        <activity android:name=".QuestionsActivity" />
        <activity android:name=".SearchableListActivity" />
    </application>

</manifest>